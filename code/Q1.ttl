?sensor <https://saref.etsi.org/core/measuresProperty> <https://dahcc.idlab.ugent.be/Homelab/SensorsAndActuators/energy.consumption>

    FILTER(?t="2022-01-03T10:57:54.000000"^^xsd:dateTime)
}